(1, 'Contact Form', ' <form action="" method="post" class="contact_form">\n                <div style="width:100%; color:red; margin-left: 25px; "></div>\n                <article>\n                    <label><h4>First name*</h4></label>\n                    <div class="info-text">\n                        <input name="first_name" value="" type="text" maxlength="32" required="required">\n                    </div>\n                </article>\n                <article>\n                    <label><h4>Last name*</h4></label>\n                    <div class="info-text">\n                        <input name="last_name" value="" type="text" maxlength="32" required="required">\n                    </div>\n                </article>\n                <article>\n                    <label><h4>Email*</h4></label>\n                    <div class="info-text">\n                        <input name="email" value="" type="text" maxlength="256" required="required">\n                    </div>\n                </article>\n                <article>\n                    <label><h4>Company</h4></label>\n                    <div class="info-text">\n                        <input name="company" value="" type="text" maxlength="256">\n                    </div>\n                </article>\n                <article>\n                    <label><h4>Phone Number</h4></label>\n                    <div class="info-text">\n                        <input name="phone" value="" type="tel" onkeypress="return ensureNumeric(event)" maxlength="20">\n                    </div>\n                </article>\n                <article class="select">\n                    <label><h4>Industry</h4></label>\n                    <div class="info-text">\n                        <select name="industry">\n                            <option value="">Please Select</option>\n                            <option value="Advertising">Advertising</option>\n                            <option value="Consulting">Consulting</option>\n                            <option value="Consumer Goods/Services3">Consumer Goods/Services</option>\n                            <option value="Design">Design</option>\n                            <option value="Education">Education</option>\n                            <option value="Entertainment">Entertainment</option>\n                            <option value="Finance">Finance</option>\n                            <option value="Government">Government</option>\n                            <option value="Human Resources">Human Resources</option>\n                            <option value="Industrial">Industrial</option>\n                            <option value="Insurance">Insurance</option>\n                            <option value="IT Consulting">IT Consulting</option>\n                            <option value="IT Development">IT Development</option>\n                            <option value="IT Other">IT Other</option>\n                            <option value="IT Support">IT Support</option>\n                            <option value="Legal">Legal</option>\n                            <option value="Logistics">Logistics</option>\n                            <option value="Media">Media</option>\n                            <option value="Medical">Medical</option>\n                            <option value="Real Estate">Real Estate</option>\n                            <option value="Science">Science</option>\n                            <option value="Security">Security</option>\n                            <option value="Telecommunication">Telecommunication</option>\n                            <option value="Travel">Travel</option>\n                        </select>\n                    </div>\n                </article>\n\n\n                <article>\n                    <label><h4>Contact Reason</h4></label>\n                    <div class="info-text">\n                        <select class="dropdown-select" name="subject">\n                            <option value="Feature Request" selected="">Feature Request</option>\n                            <option value="Bug Report">Bug Report</option>\n                            <option value="Testimonial">Testimonial</option>\n                            <option value="Inquiry">Inquiry</option>\n                            <option value="Consultation Request">Consultation Request</option>\n                            <option value="Reseller Request">Reseller Request</option>\n                            <option value="Just saying Hi">Just saying "Hi"</option>\n                        </select>\n                    </div>\n                </article>\n\n                <article class="captchabox"> <label for="code"><h4>&nbsp;</h4></label>\n                    {{request.captcha|raw}}\n                </article>\n\n                <article>\n                    <label><h4>Message*</h4></label>\n                    <div class="info-text">\n                        <textarea name="message"></textarea>\n                    </div>\n                </article>\n\n\n                <footer class="text-center">\n                    <button class="link email send_contact">Send Message</button>\n                </footer>\n            </form>', 8, 'true');

