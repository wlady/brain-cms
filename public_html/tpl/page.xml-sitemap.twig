<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
{% for route in routes %}
	{%  if route.meta.sitemap == 'true' %}
	<url>
		<loc>{{ config.MASTERDOMAIN }}{{ route.url }}</loc>
		<changefreq>weekly</changefreq>
	</url>
	{% endif %}
{%  endfor %}
</urlset>
